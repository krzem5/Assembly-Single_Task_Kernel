lib_debug={
	hash_file_path="build/hashes/lib.debug.txt"
	src_file_directory="src/lib/$NAME"
	object_file_directory="build/objects/lib_debug/"
	so_output_file_path="build/lib/lib$NAME.so"
	archive_output_file_path="build/lib/lib$NAME.a"
	includes={lib/$NAME,lib/$NAME/_generated}
	command={
		compile={
			asm="nasm -f elf64 -Wall -Werror -DBUILD_SHARED=1 -O0 -g"
			c="gcc-12 -Isrc/common/include -fno-common -fno-builtin -nostdlib -fvisibility=hidden -ffreestanding -shared -fpic -m64 -Wall -Werror -Wno-trigraphs -c -fdiagnostics-color=always -DNULL=((void*)0) -O0 -ggdb -fno-omit-frame-pointer"
		}
		link_so="ld -znoexecstack -melf_x86_64 -T src/lib/linker.ld --exclude-libs ALL -shared -O0 -g"
		archive="ar rcs"
	}
}
